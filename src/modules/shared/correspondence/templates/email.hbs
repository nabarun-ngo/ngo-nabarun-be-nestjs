                                                                                            style="font-family: 'Inter', sans-serif; text-align: start; background: #fff7ed; padding: 18px; border-radius: 8px; border-left: 4px solid #f97316; margin-bottom: 20px;">
                                                                                            <span
                                                                                                style="color: #c2410c; font-size: 15px; font-weight: 500; line-height: 1.7;">{{body.content.paragraph4_orange}}</span>
                                                                                        </div>
                                                                                        {{/if}}
                                                                                        {{# if body.content.signature}}
                                                                                        <div
                                                                                            style="font-family: 'Inter', sans-serif; text-align: inherit; margin-top: 25px; margin-bottom: 15px;">
                                                                                            <span
                                                                                                style="color: #2563eb; font-size: 15px; font-weight: 600;">{{body.content.signature}}</span>
                                                                                        </div>
                                                                                        {{/if}}
                                                                                        {{# if body.content.disclaimer}}
                                                                                        <div
                                                                                            style="font-family: 'Inter', sans-serif; text-align: center; margin-top: 25px;">
                                                                                            <div
                                                                                                style="background: #fdf4ff; padding: 18px; border-radius: 8px; border: 1px solid #e9d5ff;">
                                                                                                <span
                                                                                                    style="color: #a21caf; font-size: 14px; font-weight: 600; line-height: 1.6;">
                                                                                                    {{body.content.disclaimer}}
                                                                                                </span>
                                                                                            </div>
                                                                                        </div>
                                                                                        {{/if}}
                                                                                    </td>
                                                                                </tr>
                                                                            </tbody>
                                                                        </table>


