                                                                                        height="100%" valign="top"
                                                                                        bgcolor=""
                                                                                        role="module-content">
                                                                                        {{# if
                                                                                        body.content.paragraph3_blue}}
                                                                                        <div
                                                                                            style="font-family: 'Inter', sans-serif; text-align: start; margin-bottom: 20px;">
                                                                                            <span
                                                                                                style="color: #2563eb; font-size: 15px; font-weight: 500; line-height: 1.7;">{{body.content.paragraph3_blue}}</span>
                                                                                        </div>
                                                                                        {{/if}}
                                                                                        {{# if
                                                                                        body.content.paragraph4_orange}}
